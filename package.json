{
	"name": "uranio-trx",
	"version": "0.0.1",
	"description": "Uranio TRX - Uranio transreceiver",
	"main": "dist/server.js",
	"bin": {
		"uranio-trx-generate": "dist/server/generate.js",
		"uranio-api": "dist/api.js"
	},
	"exports": {
		".": "./dist/server.js",
		"./types": "./dist/server/types.js",
		"./client": "./dist/client.js",
		"./client/types": "./dist/client/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/server.d.ts"
			],
			"types": [
				"dist/server/types.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"client/types": [
				"dist/client/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-trx.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "yarn tsc -b",
		"dev": "yarn tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"generate": "node -r source-map-support/register -r dotenv/config ./dist/server/generate.js urn_hook_types_path=./dist/hooks/types.d.ts urn_hooks_dir_src=./.generate/src urn_hooks_dir_dist=./.generate/dist",
		"generate:schema": "yarn generate urn_command=schema",
		"generate:hook-types": "yarn generate urn_command=hook-types",
		"generate:hooks": "yarn generate urn_command=hooks"
	},
	"devDependencies": {
		"esbuild": "^0.14.22",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git#master"
	},
	"dependencies": {
		"axios": "^0.26.0",
		"dateformat": "^3",
		"dotenv": "^16.0.0",
		"uranio-api": "git+ssh://git@github.com/nbl7/uranio-api.git#master",
		"uranio-core": "git+ssh://git@github.com/nbl7/uranio-core.git#master",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git#master",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#master"
	}
}
